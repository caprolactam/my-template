{
  "name": "my-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier '**/*.{js,jsx,ts,tsx,json,css,yaml,yml}' --write",
    "lint": "eslint .",
    "tsc": "tsc -p . --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,json,yaml}": "prettier --write"
  },
  "author": "caprolactam",
  "license": "MIT",
  "devDependencies": {
    "@epic-web/config": "^1.13.0",
    "@eslint/compat": "^1.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
